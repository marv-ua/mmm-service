					ИначеЕсли ИдРеквизита = "ДебетСубконто2" Тогда
						Операция.Дебет.Субконто(2, Значение);
					ИначеЕсли ИдРеквизита = "ДебетСубконто3" Тогда
						Операция.Дебет.Субконто(3, Значение);
					ИначеЕсли ИдРеквизита = "КредитСубконто1" Тогда
						Операция.Кредит.Субконто(1, Значение);
					ИначеЕсли ИдРеквизита = "КредитСубконто2" Тогда
						Операция.Кредит.Субконто(2, Значение);
					ИначеЕсли ИдРеквизита = "КредитСубконто3" Тогда
